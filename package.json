{
  "private": true,
  "name": "ts-paeckchen-setup",
  "version": "0.0.0",
  "description": "Example setup with TypeScript and paeckchen",
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run prepare && tsc && paeckchen",
    "lint": "tslint 'src/**/*.ts' 'types/**/*.d.ts'",
    "prepare": "node scripts/prepare.js",
    "start": "http-server -o -p 1985",
    "upgrade": "ncu --upgradeAll && npm prune && npm install",
    "watch": "npm run prepare && tsc && concurrently --raw 'tsc --watch' 'paeckchen --watch'"
  },
  "dependencies": {
    "loglevel": "1.4.1"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "http-server": "0.9.0",
    "npm-check-updates": "2.8.5",
    "paeckchen-cli": "0.4.0",
    "shelljs": "0.7.4",
    "tslint": "3.15.1",
    "tslint-config-clebert": "1.0.1",
    "typescript": "2.0.3"
  }
}
